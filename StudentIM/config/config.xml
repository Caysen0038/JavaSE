        		<blue>255</blue>
        	</font-color>
        	<float-color>
        		<red>88</red>
        		<green>88</green>
        		<blue>88</blue>
        	</float-color>
        	<font-click>
        		<red>0</red>
        		<green>0</green>
        		<blue>0</blue>
        	</font-click>
        	<bg-click>
        		<red>255</red>
        		<green>255</green>
        		<blue>255</blue>
        	</bg-click>
        	<font-type>黑体</font-type>
        </theme-set>
		
	</theme>
</config>
